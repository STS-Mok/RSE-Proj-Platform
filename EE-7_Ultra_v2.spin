{{  Project: EE-7 Practical 1 - Ultrasonic  Platform: Parallax Project USB Board  Revision: 1.0  Author: Kenichi  Date: 10th Nov 2021  Log:    Date: Desc    v1    10/11/2021: Creating object file for Ultrasonic sensors & ToF sensors}}CON  ACK = 0                                                   'signals ready for more  NAK = 1                                                   'signals not ready for more  '' Reference for HC-SR04 (I2C)  UltraAdd  = $57OBJ  'bus[2]   :    "i2cDriver_v2.spin"  bus   :    "i2cDriver_v2.spin"  'bus2   :    "i2cDriver_v2.spin"PUB Init(sclPin, sdaPin)  return bus.Init(sclPin, sdaPin){PUB Init(sclPin, sdaPin, ObjNum)  'return bus[ObjNum].Init(sclPin, sdaPin)  case ObjNum    0:      return bus1.Init(sclPin, sdaPin)    1:      return bus2.Init(sclPin, sdaPin)}{PUB readSensor(ObjNum) | ackBit, clearBus, dataValue{{ Get a reading from Ultrasonic sensor }}  case ObjNum    0:      bus1.WriteByteA8(UltraAdd, $01, ackBit)      waitcnt(cnt + clkfreq/10)      dataValue := bus1.readLongA8(UltraAdd, ackBit)/254/1000      clearBus := bus1.readBus(ackBit)      return dataValue    1:      bus2.WriteByteA8(UltraAdd, $01, ackBit)      waitcnt(cnt + clkfreq/10)      dataValue := bus2.readLongA8(UltraAdd, ackBit)/254/1000      clearBus := bus2.readBus(ackBit)      return dataValue}PUB readSensor | ackBit, clearBus{{ Get a reading from Ultrasonic sensor }}  bus.WriteByteA8(UltraAdd, $01, ackBit)  waitcnt(cnt + clkfreq/10)  result := bus.readLongA8(UltraAdd, ackBit)/254/1000  clearBus := bus.readBus(ackBit)  return result